@model IQueryable<ECommerceApp7.Models.SubCategory>

<div class="row">
    <div class="col-6">
        <ul>
            <li>SubCategories</li>
            <li>
                @foreach (var sub in Model)
                {
                    <div class="container">
                        <div class="row">
                            <div class="col-3">
                                <p>@sub.SubCategoryName</p>
                            </div>
                            <div class="col-3">
                                <button id="@sub.SubCategoryId" class="remove btn btn-danger">Remove</button>
                            </div>
                        </div>
                    </div>
                }
            </li>
        </ul>
    </div>


</div>
